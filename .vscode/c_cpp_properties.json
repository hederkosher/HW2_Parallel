{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/include",
                "/opt/homebrew/opt/libomp/include",
                "/opt/homebrew/opt/open-mpi/include",
                "/opt/homebrew/Cellar/libomp/20.1.5/include",
                "/opt/homebrew/Cellar/open-mpi/5.0.7/include",
                "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/gcc/aarch64-apple-darwin24/14/include",
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include"
            ],
            "defines": [
                "_OPENMP=202011"
            ],
            "compilerPath": "/opt/homebrew/bin/gcc-14",
            "cStandard": "c99",
            "intelliSenseMode": "macos-gcc-arm64",
            "compilerArgs": [
                "-fopenmp",
                "-I/opt/homebrew/opt/libomp/include",
                "-I/opt/homebrew/opt/open-mpi/include"
            ]
        }
    ],
    "version": 4
}
